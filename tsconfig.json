{
  "compilerOptions": {
   
   
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "typeRoots": ["./node_modules/@types"]
    "moduleResolution": "node",
    "sourceMap": true,
    
    
    
    
    
  },
  "lib": ["es2020"]
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
